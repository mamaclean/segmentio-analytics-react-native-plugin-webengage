{
  "name": "segmentio-analytics-react-native-plugin-webengage",
  "version": "0.1.0",
  "description": "The hassle-free way to add WebEngage to Segment analytics in your React-Native app.",
  "main": "dist/cjs/index.js",
  "scripts": {
    "build": "bob build",
    "test": "jest",
    "typescript": "tsc --noEmit",
    "clean": "rimraf lib node_modules",
    "release": "semantic-release"
  },
  "keywords": [
    "segment",
    "react-native",
    "ios",
    "android"
  ],
  "module": "dist/esm/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "segment-analytics-react-native.podspec",
    "package.json",
    "!src/**/*.e2e.mock.js",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mamaclean/segmentio-analytics-react-native-plugin-webengage.git",
    "directory": "packages/core"
  },
  "author": "MamaClean (https://www.mamaclean.it/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mamaclean/segmentio-analytics-react-native-plugin-webengage/issues"
  },
  "homepage": "https://github.com/mamaclean/segmentio-analytics-react-native-plugin-webengage#readme",
  "dependencies": {
    "@segment/analytics-react-native": "*",
    "bob-the-bundler": "^3.0.4",
    "react-native-webengage": "^1.2.13"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^27.0.3",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "semantic-release-monorepo": "^7.0.5",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "engines": {
    "node": ">=12"
  },
  "type": "module",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}